    "def ABC(perc):\n",
    "    if perc > 0 and perc < 0.8:\n",
    "        return 'A'\n",
    "    elif perc >= 0.8 and perc < 0.95:\n",
    "        return 'B'\n",
    "    elif perc >= 0.95:\n",
    "        return 'C'"
   ]
