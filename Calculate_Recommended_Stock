    "from scipy.stats import norm\n",
    "service_levels = {'AX':.97, 'AY':.95, 'AZ':.93,'BX':.91,'BY':.90,'BZ':.89,'CX':.85,'CY':.80,'CZ':.70}\n",
    "service_factors = {k: norm.ppf(v) for k, v in service_levels.items()}\n",
