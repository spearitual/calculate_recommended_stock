   "source": [
    "sales_avg_price = sales[['Brand','SalesQuantity','SalesDollars']].groupby(['Brand']).sum().reset_index()\n",
    "sales_avg_price['avg_price'] = sales_avg_price.SalesDollars.div(sales_avg_price.SalesQuantity)"
   ]
